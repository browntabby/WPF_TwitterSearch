<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <AssemblyName>PriTrigger</AssemblyName>
    <StartupObject>PriTrigger.App</StartupObject>
    <UserSecretsId>0c70ad15-28ef-4042-9aa5-9d4fe25ccd37</UserSecretsId>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Emoji.Wpf" Version="0.2.5" />
    <PackageReference Include="Microsoft.Extensions.Configuration.UserSecrets" Version="3.1.0" />
    <PackageReference Include="Prism.Unity" Version="8.0.0.1909" />
    <PackageReference Include="ReactiveProperty" Version="7.7.0" />
    <PackageReference Include="TweetinviAPI" Version="5.0.3" />
  </ItemGroup>
  <ItemGroup>
    <None Update="settings.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>